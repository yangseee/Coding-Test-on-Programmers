#include <stdio.h>
#include <stdbool.h>
#include <stdlib.h>

int solution(int angle) {
    int answer = ((0 < angle) && (angle < 90)) ? 1 : (angle == 90) ? 2 : ((90 < angle) && (angle < 180)) ? 3 : (angle == 180) ? 4 : (printf("잘못된 각도입니다."));
    return answer;
}
