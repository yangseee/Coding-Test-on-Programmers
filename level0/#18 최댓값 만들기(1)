//https://school.programmers.co.kr/learn/courses/30/lessons/120847

#include <stdio.h>
#include <stdbool.h>
#include <stdlib.h>

// numbers_len은 배열 numbers의 길이입니다.
int solution(int numbers[], size_t numbers_len) {
    int answer = 0;
    int max = 0;
    int sub_max = 0;
    for(int i = 0; i < numbers_len; i++) {
        if(max < numbers[i]) {
            sub_max = max;
            max = numbers[i];
        }
        else {
            if(sub_max < numbers[i]) sub_max = numbers[i];
        }
    }
    return answer = max * sub_max;
}
